{{> link-navigation }} 

<div class="container p-2">    

    
    <div class="row m-4">
         <button id="btnExport" onclick="exportToExcel()">            
            <i class="fa-solid fa-floppy-disk"></i>
            Exportar tabla
        </button>
    </div>

    <div class="row">      

        <table id="tableID" class="table table-striped" data-toolbar="#toolbar">
            <thead>
                <tr>
                    <th data-sortas="case-insensitive">Nombre</th>
                    <th data-sortas="numeric">Cedula</th>
                    <th data-sortas="case-insensitive">Sigla</th>
                    <th data-sortas="numeric">Telefono</th>
                    <th data-sortas="case-insensitive">Direcci√≥n</th>
                    <th data-sortas="case-insensitive">URL</th>
                    <th>Acciones</th>
                </tr>
            </thead>
            <tbody>
                {{#each registros}}
                <tr>
                    <td>{{this.nombre}}</td>
                    <td>{{this.cedulaNIT}}</td>
                    <td>{{this.sigla}}</td>
                    <td>{{this.telefono}}</td>
                    <td>{{this.direccion}}</td>
                    <td>{{this.url}}</td>
                    <td>
                        <button><i class="fa-solid fa-eye"></i></button>
                        <button><i class="fa-solid fa-circle-plus"></i></button>
                        <button><i class="fa-solid fa-pen-to-square"></i></button>
                        <button><i class="fa-solid fa-trash-can"></i></button>
                    </td>                                    
                </tr>
                {{/each}}
                
            </tbody>
        </table>

    </div>





</div>