{{> link-navigation }}
<div class="container p-2">    

    <iframe id="txtArea1" style="display:none"></iframe>
    <div class="row m-4">
         <button id="btnExport" onclick="exportToExcel()">            
            <i class="fa-solid fa-floppy-disk"></i>
            Exportar tabla
        </button>

       
    </div>

    <div class="row">      

        <table id="tableID" class="table table-striped table-hover" data-toolbar="#toolbar">
            <thead>
                <tr>
                    <th data-sortas="case-insensitive">No. de Contrato</th>
                    <th data-sortas="numeric">AÃ±o</th>
                    <th data-sortas="case-insensitive">Objeto</th>
                    <th data-sortas="numeric">Valor Inicial</th>
                    <th data-sortas="numeric">Valor Adiciones</th>
                    <th data-sortas="numeric">Valor Final</th>
                    <th data-sortas="case-insensitive">Estado</th>
                    <th>Fecha de Firma</th>
                    <th>Fecha de Inicio</th>
                    <th>Fecha de Fin</th>
                    <th data-sortas="case-insensitive">Contratista</th>
                    <th data-sortas="case-insensitive">Subcuenta</th>
                    <th>Acciones</th>                    
                </tr>
            </thead>
            <tbody>
                {{#each registros}}
                <tr>
                    <td>{{this.NoContrato}}</td>
                    <td>{{this.anio}}</td>
                    <td>{{this.objeto}}</td>
                    <td>{{this.valorInicial}}</td>
                    <td>{{this.valoradiciones}}</td>
                    <td>{{this.valorFinal}}</td>
                    <td>{{this.estados_idestados}}</td>
                    <td>{{this.fechaFirma}}</td>
                    <td>{{this.fechaInicio}}</td>
                    <td>{{this.fechaFin}}</td>
                    <td>{{this.contratista_idcontratista}}</td>
                    <td>{{this.subcuenta_idsubcuenta}}</td>
                    <td>
                        <button><i class="fa-solid fa-eye"></i></button>
                        <button><i class="fa-solid fa-circle-plus"></i></button>
                        <button><i class="fa-solid fa-pen-to-square"></i></button>
                        <button><i class="fa-solid fa-trash-can"></i></button>
                    </td>
                </tr>
                {{/each}}
                
            </tbody>
        </table>

    </div>





</div>